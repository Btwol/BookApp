@page "/logout"
@inject IJSRuntime JSRuntime
@inject NavigationManager Navigation
@inject IAppStorage appStorage

@code {
    protected async override Task OnInitializedAsync()
    {
        await appStorage.DeleteUserFromStorage();
        await appStorage.DeleteAnalysisFromStorage();
        await appStorage.DeleteBookFromStorage();

        Navigation.NavigateTo("/redirect");
        await JSRuntime.InvokeVoidAsync("reloadPage");
    }
}
